{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "lib": ["ES2015"],
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist/local",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "typeRoots": ["./@types", "./node_modules/@types"]
  },
  "include": ["src"],
  //"exclude": ["node_modules", "src/dev"]
  "exclude": ["node_modules"]
}
